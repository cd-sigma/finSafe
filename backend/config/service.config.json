    "slack": {
      "defaultBatchProcessingLimit": 50,
      "defaultSleepTime": 5000
    },
    "push": {
      "defaultBatchProcessingLimit": 50,
      "defaultSleepTime": 5000
    },
    "email": {
      "defaultBatchProcessingLimit": 50,
      "defaultSleepTime": 5000
    }
  }
